{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Launch NestJS",
        "type": "node",
        "request": "launch",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/ts-node",
        "args": [
          "${workspaceFolder}/src/main.ts"
        ],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"]
      }
    ]
  }